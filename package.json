{
  "name": "alt-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.0.3",
    "antd-dayjs-webpack-plugin": "^0.0.9",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "react": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "tailwindcss": "^1.2.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prestart": "tailwind build ./src/baseStyles/index.tailwind.css -c ./tailwind.config.js -o ./src/baseStyles/index.css",
    "prebuild": "tailwind build ./src/baseStyles/index.tailwind.css -c ./tailwind.config.js -o ./src/baseStyles/index.css",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "./node_modules/.bin/eslint src --ext .js,.jsx",
    "fix": "./node_modules/.bin/eslint src --ext .js,.jsx --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix", 
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "prettier": "^1.19.1"
  }
}
